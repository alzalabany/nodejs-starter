    "express": "~4.16.1",
    "mariadb": "^2.5.3",
    "mongoose": "5.12.2",
    "morgan": "~1.9.1",
    "sequelize": "^6.6.2",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "concurrently": "^6.0.1",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.2.3"
